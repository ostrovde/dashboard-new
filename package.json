{
  "name": "dashboard-new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "diag": "python3 scripts/diagnostics.py",
    "routes:check": "python3 scripts/vite_validate_routes.py",
    "smoke": "node scripts/smoke.cjs",
    "serve:dist": "http-server dist -p 8081 --silent",
    "smoke:local": "cross-env STAGE_URL=http://127.0.0.1:8081 npm run smoke",
    "build+smoke": "npm run build && npm run postbuild && npm run prestartsmoke && start-server-and-test \"npm run serve:dist\" http://127.0.0.1:8081 \"npm run smoke:local\"",
    "postbuild": "node scripts/postbuild-ensure-map-root.cjs",
    "prestartsmoke": "kill-port 8081 || true"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "http-server": "^14.1.1",
    "kill-port": "^2.0.1",
    "start-server-and-test": "^2.1.2",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
